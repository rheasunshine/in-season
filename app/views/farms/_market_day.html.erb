<li>
  <h6 style="font-size:125%;"><%= market_day %></h6>
  <% market_day_markets = @markets.where(market_day: market_day) %>
    <% if market_day_markets.length == 0 %>
      We have no markets scheduled on <%= market_day %>s.<br /><br />
    <% end %>
  <% market_day_markets.map do |market| %>
  <ul>
    <span>
    <% if current_user && current_user.id == @farm.user_id %>
    <%= link_to farm_remove_market_path(farm_id: @farm.id, market: market), method: :delete do %>
    <i style="float:left;line-height:1.5;" class="trash-bin material-icons">delete</i>
    <% end %>
    <% end %>
  </span>
  <div style="display:inline-block">

    <li>
      <%= market.name %>
    </li>
    <li>
      <%= market.parsed_address %><br /><br />
    </li>
  </div>
  </ul>
  <% end %>
</li>
